{
    "$schema": "https://railway.app/railway.schema.json",
    "name": "ITSM Dashboard",
    "plugins": [
        {
            "name": "streamlit",
            "service": "streamlit",
            "variables": {
                "PORT": "8000",
                "STREAMLIT_SERVER_PORT": "8000",
                "STREAMLIT_SERVER_HEADLESS": "true",
                "STREAMLIT_SERVER_ENABLE_CORS": "false",
                "STREAMLIT_SERVER_ENABLE_WEBSOCKET_ORIGIN_CHECK": "false"
            }
        }
    ]
}

